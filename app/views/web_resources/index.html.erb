<h1>Listing web_resources</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Resourceable</th>
    <th>Resourceable type</th>
    <th>Resource type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @web_resources.each do |web_resource| %>
  <tr>
    <td><%= web_resource.url %></td>
    <td><%= web_resource.resourceable_id %></td>
    <td><%= web_resource.resourceable_type %></td>
    <td><%= web_resource.resource_type %></td>
    <td><%= link_to 'Show', web_resource %></td>
    <td><%= link_to 'Edit', edit_web_resource_path(web_resource) %></td>
    <td><%= link_to 'Destroy', web_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Web resource', new_web_resource_path %>
