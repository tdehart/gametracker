<%= simple_form_for @tournament do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <%= f.input :name %>
  <%= f.association :game %>
  <!-- <%= f.association :game, :url => autocomplete_game_name_tournaments_path, as: :autocomplete %> -->
  <%= f.input :link, label: 'Official Website' %>
  <%= f.input :start_date, as: :date_picker %>
  <%= f.input :end_date, as: :date_picker %>
  <%= f.input :prize_pool_cents, label: 'Prize Pool' %>
  <%= f.input :currency_iso_code %>
  <%= f.input :region %>
  <%= f.input :image, as: :file %>
  <%= f.input :remote_image_url%>
  <%= f.button :submit %>
<% end %>